{
  "name": "11ty-tailwind-starter",
  "version": "1.0.0",
  "description": "A 11ty (eleventy) static site generator with Tailwind CSS starter template.",
  "scripts": {
    "start": "eleventy --serve & npx tailwindcss -i src/styles.css -o dist/styles.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production npx tailwindcss -i src/styles.css -o dist/styles.css --minify"
  },
  "keywords": [
    "static-site-generator",
    "static-site",
    "ssg",
    "eleventy",
    "11ty",
    "tailwind",
    "tailwindcss",
    "nunjucks",
    "njk"
  ],
  "author": {
    "name": "Jak Wilkins",
    "email": "jak@wlkns.co",
    "url": "https://wlkns.co/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wlkns/11ty-tailwind-starter.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "tailwindcss": "^3.0.23"
  }
}
